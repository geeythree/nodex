{
  "name": "secureai-backend",
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "CREWAI_MODEL": "@crewai_model"
  }
}